    "edata": "^0.9.5"
  },
  "devDependencies": {
    "@types/node": "^12.0.7",
    "rimraf": "^2.6.3",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
